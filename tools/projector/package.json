{
  "name": "eo-projector",
  "version": "1.0.0",
  "description": "Event-sourced projection engine: fetches Matrix events, replays them, renders static HTML",
  "type": "module",
  "scripts": {
    "build": "tsc && node dist/index.js",
    "fetch": "tsc && node dist/index.js --fetch-only",
    "replay": "tsc && node dist/index.js --replay-only",
    "render": "tsc && node dist/index.js --render-only",
    "dev": "tsx src/index.ts"
  },
  "dependencies": {
    "jsonpatch": "^3.0.1",
    "marked": "^12.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
